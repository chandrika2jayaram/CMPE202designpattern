package edu.sjsu.library.template;

public class Update extends MemberTemplate {


	
public void borrow(int numItem, int numBorrow){
		
		System.out.println("Update with item borrowed");
		numItem = numItem + numBorrow;
		//return numItem;
		//System.out.println("items borrowed : " + (numItem + numBorrow));
			
	}
	
	
	void renew(int numItem, int numRenew){
		System.out.println("Update item Renewed ");
		numItem = numItem - numRenew;
		//return numRenew;
		
	}
	
	void returnItem(int numItem, int numReturn) {
		System.out.println("Update item returned");
		
	}
	
	void update(int numItem, int numBorrow, int numReturn) {
				
		System.out.println("update member account with items borrowed, renewed and returned" );
		//return numItem;
				
	}


	
	
}
